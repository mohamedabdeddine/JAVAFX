<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="301.0" prefWidth="331.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo2.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
        <children>
            <Label prefHeight="17.0" prefWidth="98.0" text="Nom" />
            <TextField fx:id="nomTextField" prefHeight="14.0" prefWidth="292.0" />
        </children>
    </HBox>
    <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
        <children>

            <Label text="Date de création" />
            <SplitPane dividerPositions="0.29797979797979796" opacity="0.0" prefHeight="55.0" prefWidth="14.0">
                <items>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="53.0" prefWidth="0.0" />
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="53.0" prefWidth="0.0" />
                </items>
            </SplitPane>
            <DatePicker fx:id="datePicker" />
        </children>
    </HBox>
    <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
        <children>

            <Label text="Entraîneur" />
            <SplitPane opacity="0.0" prefHeight="41.0" prefWidth="16.0" />
            <TextField fx:id="entraineurTextField" prefHeight="26.0" prefWidth="218.0" />
        </children>
    </HBox>
    <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
        <children>

            <Button onAction="#onHelloButtonClick" text="Ajouter" />
            <SplitPane opacity="0.0" prefHeight="41.0" prefWidth="30.0" />
            <Button onAction="#onAnnulerButtonClick" text="Annuler" />
        </children>
        <opaqueInsets>
            <Insets />
        </opaqueInsets>
        <effect>
            <Glow level="0.0" />
        </effect>
    </HBox>

    <Label fx:id="welcomeText" />
</VBox>